include README.md

include setupbase.py

include tsconfig.json
include package.json
include webpack.config.js
include jupyter_jsmol/labextension/*.tgz

# Documentation
graft docs
exclude docs/\#*
prune site

# Examples
graft examples

# Tests
graft tests
prune tests/build

# Javascript files
graft jupyter_jsmol/nbextension
graft src
graft css
prune **/node_modules
prune coverage
prune lib

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
